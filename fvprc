    export FVP_ARCHIVE=${FVP_ARCHIVE:-"avh-linux-aarch64_11.24_24_Linux64_armv8l.tgz"}
else
    export FVP_ARCHIVE=${FVP_ARCHIVE:-"avh-linux-x86_11.24_24_Linux64.tgz "}
fi
